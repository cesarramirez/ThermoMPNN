{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: ThermoMPNN",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/train_thermompnn.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
